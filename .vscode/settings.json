{
    "editor.minimap.enabled": false,
    "diffEditor.wordWrap": "on",
    "editor.wordWrap": "on",
    "editor.inlineSuggest.enabled": true,
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "github.copilot.enable": {
        "*": true,
        "yaml": false,
        "plaintext": true,
        "markdown": true,
        "javascript": true,
        "json": true,
        "typescript": true,
        "javascriptreact": false,
        "typescriptreact": true
    },
    "javascript.updateImportsOnFileMove.enabled": "always",
    "editor.mouseWheelScrollSensitivity": 2,
    "workbench.list.mouseWheelScrollSensitivity": 2,
    "terminal.integrated.mouseWheelScrollSensitivity": 2,
    "workbench.tree.indent": 20,
    "prettier.configPath": ".prettierrc",
    "material-icon-theme.activeIconPack": "react_redux",
    "workbench.iconTheme": "material-icon-theme",
    "quokka.automaticRestart": true,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "omnisharp.dotnetPath": "latest",
    "editor.fontFamily": "Cascadia Code, Consolas",
    "eslint.workingDirectories": [
        "./backend",
        "./frontend"
    ],
    "workbench.startupEditor": "none",
    "tabnine.experimentalAutoImports": true,
    "liveSassCompile.settings.showOutputWindowOn": "Warning",
    "editor.codeLensFontFamily": "Fira Code",
    "kite.showWelcomeNotificationOnStartup": false,
    "editor.scrollbar.horizontalScrollbarSize": 15,
    "editor.scrollbar.verticalScrollbarSize": 10,
    "editor.fontSize": 13,
    "workbench.colorTheme": "Bearded Theme feat. Will",
    "notebook.consolidatedRunButton": true,
    "markdown-preview-enhanced.previewTheme": "github-dark.css",
    "extensions.ignoreRecommendations": true,
    "totalTypeScript.hideAllTips": false,
    "totalTypeScript.hideBasicTips": false,
    "explorer.confirmDelete": false,
    "material-icon-theme.files.color": "#26a69a",
    "tailwindCSS.includeLanguages": {
        "html": "html",
        "javascript": "javascript",
        "css": "css",
        "plaintext": "html"
    },
    "files.associations": {
        "*.css": "tailwindcss"
    },
    "editor.quickSuggestions": {
        "strings": true
    },
    "git.openRepositoryInParentFolders": "never",
    "window.commandCenter": false,
    "workbench.layoutControl.enabled": false,
    "discord.removeLowerDetails": true,
    "workbench.preferredLightColorTheme": "Dainty â€“ Material Theme Ocean",
    "errorLens.enabledDiagnosticLevels": [
        "error",
        "info"
    ],
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit",
        "source.fixAll": "explicit"
    },
    "eslint.format.enable": true,
    "prettier.enable": false,
    "editor.formatOnSave": true,
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
    ]
}